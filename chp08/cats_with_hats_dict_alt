cats = {}
for i in range(1, 101):
    cats[i] = 1

for i in range(2, 101):
    count = i
    while count < 101:
        if cats[count] == 0:
            cats[count] = 1
        elif cats[count] == 1:
            cats[count] = 0
        count += i

for cat, hats in cats.items():
    print(cat, "==========>", hats)
